{
  "name": "axiom-agent-factory",
  "main": "src/index.ts",
  "compatibility_date": "2025-11-19",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "routes": [
    {
      "pattern": "brain.axiomid.app",
      "custom_domain": true
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "AGENT_DO",
        "class_name": "AgentDO",
        "script_name": "axiom-agent-do"
      }
    ]
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "axiom-brain-db",
      "database_id": "44eab8d6-1201-4414-9626-effdefb05eae"
    }
  ],
  "services": [
    {
      "binding": "GEMINI_ROUTER",
      "service": "axiom-gemini-router"
    },
    {
      "binding": "TOOL_EXECUTOR",
      "service": "axiom-tool-executor"
    },
    {
      "binding": "AUTH_WORKER",
      "service": "axiom-auth-worker"
    }
  ]
}