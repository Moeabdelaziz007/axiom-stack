{
  "name": "axiom-tool-executor",
  "main": "src/index.ts",
  "compatibility_date": "2025-11-19",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "services": [
    {
      "binding": "AUTH_WORKER",
      "service": "axiom-auth-worker"
    },
    {
      "binding": "HEALTH_AGENT",
      "service": "axiom-health-agent"
    },
    {
      "binding": "TRAVEL_AGENT",
      "service": "axiom-travel-agent"
    },
    {
      "binding": "NLP_AGENT",
      "service": "axiom-nlp-agent"
    },
    {
      "binding": "BIGQUERY_AGENT",
      "service": "axiom-bigquery-agent"
    }
  ],
  "custom_domain": true,
  "vars": {
    "GOOGLE_CLOUD_PROJECT_ID": "",
    "GOOGLE_TRANSLATE_API_KEY": "",
    "GOOGLE_SPEECH_API_KEY": "",
    "BIRDEYE_API_KEY": "",
    "HELIUS_API_KEY": ""
  }
}